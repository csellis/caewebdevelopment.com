  },
  "homepage": "https://github.com/taylorbryant/next-starter-tailwind#readme",
  "dependencies": {
    "@statickit/react": "^2.1.1",
    "@tailwindcss/ui": "^0.7.2",
    "autoprefixer": "^10.0.2",
    "next": "^10.0.3",
    "next-seo": "^4.17.0",
    "react": "16.14.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
